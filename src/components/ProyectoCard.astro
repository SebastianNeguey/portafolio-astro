--- 
import {Astro} from 'astro'
const { titulo, descripcion, imagenUrl, link } = Astro.props;
const imagenConBase = `${Astro.base}${imagenUrl}`;
---

<div class="proyecto">
  <img src={imagenConBase} alt={titulo} /> 
  <h3>{titulo}</h3>
  <h3>{Astro.base}</h3>
  <h3>{imagenUrl}</h3>
  <h3>{imagenConBase}</h3>
  <p>{descripcion}</p>
  <a href={link} target="_blank">Ver m√°s</a>
</div>
